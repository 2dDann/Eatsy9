<!-- rounded_rectangle.xml -->
<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="rectangle">
    <solid android:color="#FFFFFF" />
    <corners android:radius="10dp" /> <!-- Adjust the radius for the desired roundness -->
    <stroke
        android:width="1dp"
        android:color="#CCCCCC" /> <!-- Adjust the stroke color and width as needed -->
</shape>
